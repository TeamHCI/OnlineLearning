<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hci.onilearn.onilearn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ListMonHoc" />
        <activity
            android:name=".activity.DatLichActivity"
            android:parentActivityName=".activity.MainActivity" />
        <activity android:name=".activity.ThongKeActi" />
        <activity
            android:name=".activity.TrueFalseActivity"
            android:parentActivityName=".activity.MainActivity" />
        <activity android:name=".activity.FalseActivity" />
        <activity android:name=".activity.TrueActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AlarmActivity" />

        <receiver
            android:name=".receiver.AlarmReceiver"
            android:enabled="true"
            android:exported="true" />

        <service
            android:name=".service.RingToneService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".activity.SubjectChoiceActivity"
            android:label="Choose a subject"
            android:parentActivityName=".activity.DatLichActivity" />
        <activity android:name=".activity.ResultActivity"></activity>
    </application>

</manifest>